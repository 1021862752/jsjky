.userinfo {

  display: flex;
  flex-direction: column;
    align-items: center;

  border-radius: 10px;
  border: 1rpx lightblue solid;
  background: lightblue;
  margin: 30rpx;
  
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  /* border-radius: 50%; */
}

.setup-list {
        padding: 0 30rpx;
}
.setup-list navigator {
        display: flex;
     
         padding: 30rpx 0;
         border-bottom: 1rpx solid #c4c4c4;
         border-radius: 10px;
         margin-top: 10px;
        align-items: center;
        justify-content: center;
      
}
.setup-list navigator text{
        font-size: 35rpx;
}
.setup-list navigator image{
        height: 35rpx;
        width: 35rpx;
}
.quit {
      padding: 30rpx ;
}
.quit text{
        width: 100%;
        padding: 18rpx 0;
        background-color: rgba(217, 42, 42, 1);
        display: flex;
        font-size: 30rpx;
        color: #fff;
        align-items: center;
        justify-content: center;
        border-radius: 10rpx;
}